{
  "name": "@qcrypt/monorepo",
  "version": "7.0.0",
  "type": "module",
  "copyright": "© 2024–2026 Quick Crypt Contributors",
  "scripts": {
    "ng": "ng",
    "serve": "nx serve --hmr false --host 0.0.0.0 --watch --poll 1000 --ssl --ssl-cert apps/qcrypt/localssl/localhost.crt --ssl-key apps/qcrypt/localssl/localhost.key",
    "build": "nx build qcrypt --configuration production --subresource-integrity",
    "test": "nx test",
    "test:watch": "nx test --watch",
    "test:all": "nx test --runnerConfig=apps/qcrypt/vitest-all.config.ts",
    "test:e2e": "NODE_EXTRA_CA_CERTS=./apps/qcrypt/localssl/qcrypt.pem pnpm exec playwright test --config apps/qcrypt/playwright.config.ts --project=local --workers=1 apps/qcrypt/tests/sequential && pnpm exec playwright test --config apps/qcrypt/playwright.config.ts --project=local apps/qcrypt/tests/parallel --grep-invert \"/@fullfuzz/\"",
    "test:fuzz": "NODE_EXTRA_CA_CERTS=./apps/qcrypt/localssl/qcrypt.pem pnpm exec playwright test --config apps/qcrypt/playwright.config.ts --project=local apps/qcrypt/tests/parallel --grep \"/@fullfuzz/\"",
    "test:e2e:prod": "pnpm exec playwright test --config apps/qcrypt/playwright.config.ts --project=prod --workers=1 apps/qcrypt/tests/sequential && pnpm exec playwright test --config apps/qcrypt/playwright.config.ts --project=prod apps/qcrypt/tests/parallel --grep-invert \"/@fullfuzz/\"",
    "test:fuzz:prod": "pnpm exec playwright test --config apps/qcrypt/playwright.config.ts --project=prod apps/qcrypt/tests/parallel --grep \"/@fullfuzz/\"",
    "serve:ssr:qcrypt": "node dist/qcrypt/server/server.mjs",
    "build:cli": "nx build qcrypt-cli",
    "build:cli:min": "nx build-min qcrypt-cli",
    "build:server": "nx build qcrypt-server",
    "build:server:min": "nx build-min qcrypt-server",
    "test:server": "nx test qcrypt-server",
    "test:server:prod": "nx test-prod qcrypt-server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "^21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/material": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/platform-server": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@angular/ssr": "^21.1.3",
    "@scure/bip39": "^2.0.1",
    "@aws-sdk/client-dynamodb": "^3.840.0",
    "@aws-sdk/client-kms": "^3.840.0",
    "@simplewebauthn/browser": "^13.0.0",
    "@simplewebauthn/server": "^13.0.0",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "@zxcvbn-ts/language-es-es": "^3.0.2",
    "@zxcvbn-ts/matcher-pwned": "^3.0.4",
    "@inquirer/prompts": "^7.10.0",
    "electrodb": "^3.4.0",
    "jsonwebtoken": "^9.0.2",
    "libsodium-wrappers": "^0.8.0",
    "luxon": "^3.6.2",
    "reopen-tty": "^1.1.2",
    "rxjs": "~7.8.0",
    "tar": "^7.5.2",
    "tslib": "^2.3.0",
    "yargs": "^17.7.2",
    "urlpattern-polyfill": "^10.1.0",
    "xss": "^1.0.15",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.840.0",
    "@angular-devkit/core": "^21.1.3",
    "@angular-devkit/schematics": "^21.1.3",
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@nx/angular": "22.4.5",
    "@nx/workspace": "22.4.5",
    "@playwright/test": "^1.55.1",
    "@schematics/angular": "^21.1.3",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/luxon": "^3.6.2",
    "@types/node": "^24.10.7",
    "@types/yargs": "^17.0.34",
    "esbuild": "^0.27.0",
    "@vitest/browser-playwright": "^4.0.18",
    "chromium-bidi": "^13.1.1",
    "devtools-protocol": "^0.0.1551306",
    "jsdom": "^28.0.0",
    "nid-webauthn-emulator": "^0.2.8",
    "nx": "22.4.5",
    "schematics-scss-migrate": "^2.3.17",
    "tsx": "^4.20.0",
    "typescript": "~5.9.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "@modelcontextprotocol/sdk": "1.26.0"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "less",
      "lmdb",
      "msgpackr-extract",
      "nx"
    ]
  }
}
